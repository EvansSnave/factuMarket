<%= form_with url: "/facturas", method: :get, local: true do |f| %>
  <div>
    <%= f.label :fechaInicio, "Desde" %>
    <%= f.date_field :fechaInicio %>
  </div>
  <div>
    <%= f.label :fechaFin, "Hasta" %>
    <%= f.date_field :fechaFin %>
  </div>

  <div>
    <%= f.submit "Buscar facturas" %>
  </div>
<% end %>
